<terminal>
    <!--
      ANDROID_LOG_UNKNOWN = 0
      ANDROID_LOG_DEFAULT = 1
      ANDROID_LOG_VERBOSE = 2
      ANDROID_LOG_DEBUG   = 3
      ANDROID_LOG_INFO    = 4
      ANDROID_LOG_WARN    = 5
      ANDROID_LOG_ERROR   = 6
      ANDROID_LOG_FATAL   = 7
      ANDROID_LOG_SILENT  = 8
    -->
    <use-sdk
        version="1.0"
        loglevel="5" />

    <device
        type="idcard_reader"
        model="auto-detect"
        forced="false" >
        <uart
            path="/dev/ttyS0"
            baudrate="115200" />
    </device>
</terminal>